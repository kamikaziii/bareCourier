{
	"name": "barecourier",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"convert-logo": "node scripts/convert-logo.mjs",
		"copy-pwa-assets": "node scripts/copy-pwa-assets.mjs",
		"generate-pwa-assets": "node scripts/convert-logo.mjs && pwa-assets-generator && node scripts/copy-pwa-assets.mjs",
		"types:generate": "npx supabase gen types typescript --project-id $PROJECT_ID > src/lib/database.generated.ts",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@internationalized/date": "^3.10.1",
		"@lucide/svelte": "^0.561.0",
		"@playwright/test": "^1.58.0",
		"@sveltejs/adapter-vercel": "^6.3.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.18",
		"@types/mapbox-gl": "^3.4.1",
		"@vite-pwa/assets-generator": "^1.0.2",
		"@vite-pwa/sveltekit": "^1.1.0",
		"dotenv": "^17.2.4",
		"eslint": "^9.39.2",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.3.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"sharp": "^0.34.5",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"svelte-sonner": "^1.0.7",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.2.6",
		"vite-plugin-pwa": "^1.2.0",
		"workbox-cacheable-response": "^7.4.0",
		"workbox-expiration": "^7.4.0",
		"workbox-precaching": "^7.4.0",
		"workbox-routing": "^7.4.0",
		"workbox-strategies": "^7.4.0",
		"workbox-window": "^7.4.0"
	},
	"dependencies": {
