		"bits-ui": "^2.15.4",
		"chart.js": "^4.5.1",
		"clsx": "^2.1.1",
		"idb-keyval": "^6.2.2",
		"mapbox-gl": "^3.18.0",
		"qrcode": "^1.5.4",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"workbox-background-sync": "^7.4.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"sharp"
		]
	}
